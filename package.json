  "devDependencies": {
    "@types/node": "18.11.11",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "8.5.0",
    "prettier": "2.8.1",
    "typescript": "4.9.4"
  }
}
